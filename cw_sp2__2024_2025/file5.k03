NAME _prPR ; 
BODY DATA INTEGER16 _aaAA , _bbBB , _xxXX , _yyYY ;
  GET ( _aaAA )
  GET ( _bbBB )
  
  PUT ( _aaAA + _bbBB )
  PUT ( _aaAA - _bbBB )
  PUT ( _aaAA * _bbBB )
  PUT ( _aaAA DIV _bbBB )
  PUT ( _aaAA MOD _bbBB )

  _xxXX := (_aaAA - _bbBB) * 10 + (_aaAA + _bbBB) DIV 10
  _yyYY := _xxXX + _xxXX MOD 10

  PUT ( _xxXX )
  PUT ( _yyYY )

END